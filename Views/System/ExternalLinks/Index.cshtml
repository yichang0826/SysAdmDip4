@model IEnumerable<SysAdmDip4.Models.System.ExternalLink>

@{
    ViewData["Title"] = "Index";
    Layout = "_LayoutAdmin";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Link_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Link_Url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Link_IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Link_CreateDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Link_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Link_Url)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Link_IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Link_CreateDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Link_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Link_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Link_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
